<?php

class Mconnectsolutiions_Bestsellerproducts_Model_Autoplaysilder 
{
    public function toOptionArray()
    {
        return array(
            array('value' => 'false', 'label'=>Mage::helper('adminhtml')->__('No')),
            array('value' => 'true', 'label'=>Mage::helper('adminhtml')->__('Yes')),
            
        );
    }
}
